alias t="touch tmp/restart.txt"
alias composer="php /usr/local/bin/composer.phar"
alias p=python3
alias restart-postgres="rm /usr/local/var/postgres/postmaster.pid; launchctl unload ~/Library/LaunchAgents/homebrew.mxcl.postgresql.plist; launchctl load -w ~/Library/LaunchAgents/homebrew.mxcl.postgresql.plist"
alias workoff=deactivate

# Get my Github project stats
alias gs="curl -sG https://api.github.com/search/repositories --data-urlencode 'q=@jemcode @rikkipitt' -H 'Accept: application/vnd.github.preview' | jq '.items[] | {html_url, watchers_count, forks_count}'"
